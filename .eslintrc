{
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "globals": {
    "moment": "readonly",
    "module": "readonly",
    "process": "readonly",
    "require": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "plugins": ["react"],
  "rules": {
    "react/display-name": 0,
    "react/prop-types": 0,
    "no-console": "warn"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
